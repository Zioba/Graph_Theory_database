sc_node_not_relation->concept_directed_graph;;

concept_directed_graph	=> nrel_main_idtf: 
		[ориентированный граф] (* <-lang_ru;; *);
		[directed graph] (* <-lang_en;; *);;


concept_directed_graph <- rrel_key_sc_element:
		...  
		(*<-sc_definition;;
		<=nrel_sc_text_translation:
		...
			(*
			-> rrel_example: 
				[Ориентированный граф - граф, рёбрам которого присвоено направление.] (*<-lang_ru;;*);;
			*);;
		*);;

concept_directed_graph <- rrel_key_sc_element: 
	...
	(*
		<- sc_illustration;;
		<= nrel_sc_text_translation:
		...
			(*
			-> rrel_example:
				"file://contents/directed_graph.png";;
			*);;
	*);;

concept_directed_graph <=> nrel_antipode: concept_undirected_graph;;

concept_graph => nrel_inclusion: concept_directed_graph;;
concept_graph => nrel_inclusion: concept_undirected_graph;;
