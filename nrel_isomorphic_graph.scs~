nrel_isomorphic_graph
	=> nrel_main_idtf: 
		[ изоморфный граф* ] (* <-lang_ru;; *);
		[ isomorphic graph* ] (* <-lang_en;; *);

	->rrel_key_sc_element:
		...  
		(*<-sc_definition;;
	=> nrel_main_idtf: [Опр.(изоморфный граф)] (* <- lang_ru;; *);;
		<=nrel_sc_text_translation:
		...
			(*
				-> rrel_example: 
				[Граф G=<Vg,Eg> называется изоморфным графу H=<Vh,Eh> , если между  множествами их вершин существует биекция f: Vg в Vh, такая что любые две вершины u и v графа G смежны тогда и только тогда , когда вершины f(u) и f(v) смежны в графе.] (*<-lang_ru;;*);;
			*);;
		*);
	
			
	 <-rrel_key_sc_element: ...
	(*
		=>nrel_main_idtf:[Утв.(изоморфизм) ](*<-lang_ru;;*);;
		<- sc_statement;;	
		<=nrel_sc_text_translation:
		...
			(*
				-> rrel_example: [Изоморфизм это отношение эквивалентности на графах] (*<-lang_ru;;*);;
			*);;
		*);
			... (*
		=>nrel_main_idtf:[Утв.(автоморфизм) ](*<-lang_ru;;*);;
		<- sc_statement;;	
		<=nrel_sc_text_translation:
		...
			(*
				-> rrel_example: [В случае если биекия отображает граф сам на себя (графы G и H совпадают ) , то она называется автоморфизмом графов.]  (*<-lang_ru;;*);;
			*);;
		
	*);

 => nrel_definitional_domain: 
