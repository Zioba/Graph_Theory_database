				(* 	<- definition;;
					<= nrel_sc_text_translation:...
					(*
						->rrel_example:[Висячая вершина - вершина, степень которой равна 1.]
						(*<- lang_ru;;*);;
						<= nrel_using_constants: concept_vertex;;
					*);;
				*);;

